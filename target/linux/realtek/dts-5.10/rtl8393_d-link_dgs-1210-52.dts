		SWITCH_PORT(46, 47, qsgmii)
		SWITCH_PORT(47, 48, qsgmii)

#ifdef DGS1210_52_TEST_SFP
		COMBO_PORT(48, 49, qsgmii)
		COMBO_PORT(49, 50, qsgmii)
		COMBO_PORT(50, 51, qsgmii)
		COMBO_PORT(51, 52, qsgmii)
#endif
		/* CPU-Port */
		port@52 {
			ethernet = <&ethernet0>;
			reg = <52>;
			phy-mode = "qsgmii";
			fixed-link {
				speed = <1000>;
				full-duplex;
			};
		};
	};
