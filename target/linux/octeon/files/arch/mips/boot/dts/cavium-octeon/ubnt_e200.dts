/dts-v1/;

/ {
	compatible = "ubnt,e200";
	interrupt-parent = <0x01>;
	model = "ubnt,e200";
	#address-cells = <0x02>;
	#size-cells = <0x02>;

	memory {
		reg = <0x00 0x00 0x00 0x10000000 0x00 0x20000000 0x00 0x70000000>;
		device_type = "memory";
