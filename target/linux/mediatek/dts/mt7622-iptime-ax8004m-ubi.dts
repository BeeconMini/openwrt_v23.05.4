/dts-v1/;
#include "mt7622-iptime-ax8004m.dtsi"

/ {
	model = "ipTIME AX8004M (UBI)";
	compatible = "iptime,ax8004m-ubi", "mediatek,mt7622";
};

&snand {
	partitions {
		partition@200000 {
			label = "firmware1";
			reg = <0x0200000 0x2000000>;
			read-only;
		};

		partition@2200000 {
			label = "kernel";
			reg = <0x2200000 0x0400000>;
		};

		partition@2600000 {
			label = "ubi";
			reg = <0x2600000 0x4C00000>;
		};
	};
};

