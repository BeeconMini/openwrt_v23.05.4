#include "qca9563_dlink_dir-8x9-a1.dtsi"

/ {
	model = "D-Link DIR-859 A1";
	compatible = "dlink,dir-859-a1", "qca,qca9563";

	aliases {
		led-boot = &led_power;
		led-failsafe = &led_power;
		led-running = &led_power;
		led-upgrade = &led_power;
	};

	leds {
		compatible = "gpio-leds";

		wps {
			label = "green:wps";
			gpios = <&gpio 8 GPIO_ACTIVE_LOW>;
		};

		led_power: power {
			label = "green:power";
			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
		};

		internet {
			label = "green:internet";
			gpios = <&gpio 16 GPIO_ACTIVE_LOW>;
		};

		wlan {
			label = "green:wlan";
			gpios = <&gpio 19 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0tpt";
		};
	};
};

&ath10k {
	nvmem-cells = <&calibration_ath10k>, <&macaddr_devdata_94 0>;
	nvmem-cell-names = "calibration", "mac-address";
};

&devdata {
	compatible = "nvmem-cells";

	nvmem-layout {
		compatible = "fixed-layout";
		#address-cells = <1>;
		#size-cells = <1>;

		macaddr_devdata_79: macaddr@79 {
			compatible = "mac-base";
			reg = <0x79 0x11>;
			#nvmem-cell-cells = <1>;
		};

		macaddr_devdata_94: macaddr@94 {
			compatible = "mac-base";
			reg = <0x94 0x11>;
			#nvmem-cell-cells = <1>;
		};

		macaddr_devdata_b0: macaddr@b0 {
			compatible = "mac-base";
			reg = <0xb0 0x11>;
			#nvmem-cell-cells = <1>;
		};

		macaddr_devdata_c9: macaddr@c9 {
			compatible = "mac-base";
			reg = <0xc9 0x11>;
			#nvmem-cell-cells = <1>;
		};
	};
};

&lan1 {
	nvmem-cells = <&macaddr_devdata_c9 0>;
	nvmem-cell-names = "mac-address";
};

&lan2 {
	nvmem-cells = <&macaddr_devdata_c9 0>;
	nvmem-cell-names = "mac-address";
};

&lan3 {
	nvmem-cells = <&macaddr_devdata_c9 0>;
	nvmem-cell-names = "mac-address";
};

&lan4 {
	nvmem-cells = <&macaddr_devdata_c9 0>;
	nvmem-cell-names = "mac-address";
};

&wan {
	nvmem-cells = <&macaddr_devdata_79 0>;
	nvmem-cell-names = "mac-address";
};

&wmac {
	nvmem-cells = <&calibration_ath9k>, <&macaddr_devdata_b0 0>;
	nvmem-cell-names = "calibration", "mac-address";
};
