/dts-v1/;

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

#include "ar9132.dtsi"

/ {
	compatible = "buffalo,wzr-hp-g300nh", "qca,ar9132";
	model = "Buffalo WZR-HP-G300NH";

	aliases {
		led-boot = &security;
		led-failsafe = &diag;
		led-upgrade = &diag;
	};

	clock40mhz: ref {
		compatible = "fixed-clock";
		#clock-cells = <0>;
		clock-frequency = <40000000>;
	};

	gpio2: gpio-extender {
		compatible = "nxp,74hc153-gpio";
		gpio-controller;
		ngpios = <8>;
		#gpio-cells = <2>;

		// GPIOs used by this node
		gpio-s0 = <&gpio 9 GPIO_ACTIVE_HIGH>;
		gpio-s1 = <&gpio 11 GPIO_ACTIVE_HIGH>;
		gpio-1y = <&gpio 12 GPIO_ACTIVE_HIGH>;
		gpio-2y = <&gpio 14 GPIO_ACTIVE_HIGH>;
	};

	keys {
		compatible = "gpio-keys-polled";

		poll-interval = <20>;

		aoss {
			label = "aoss";
			linux,code = <KEY_WPS_BUTTON>;
			gpios = <&gpio2 0 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio2 1 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};

		router_on {
			label = "router_on";
			linux,code = <BTN_5>;
			gpios = <&gpio2 2 GPIO_ACTIVE_HIGH>;
			debounce-interval = <60>;
		};

		qos_on {
			label = "qos_on";
			linux,code = <BTN_3>;
			gpios = <&gpio2 3 GPIO_ACTIVE_HIGH>;
			debounce-interval = <60>;
		};

		usb {
			label = "usb";
			linux,code = <BTN_2>;
			gpios = <&gpio2 5 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};

		router_auto {
			label = "router_auto";
			linux,code = <BTN_6>;
			gpios = <&gpio2 6 GPIO_ACTIVE_HIGH>;
			debounce-interval = <60>;
		};

		qos_off {
			label = "qos_off";
			linux,code = <BTN_4>;
			gpios = <&gpio2 7 GPIO_ACTIVE_HIGH>;
			debounce-interval = <60>;
		};
	};

	flash@1e000000 {
		compatible = "cfi-flash";
		reg = <0x1e000000 0x2000000>;
		bank-width = <2>;
		device-width = <2>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x0000000 0x0040000>;
				read-only;
			};

			partition@40000 {
				label = "u-boot-env";
				reg = <0x0040000 0x0020000>;
				read-only;
			};

			partition@60000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x0060000 0x1f60000>;
			};

			partition@1fc0000 {
				label = "user_property";
				reg = <0x1fc0000 0x0020000>;
				read-only;
			};

			art: partition@1fe0000 {
				label = "art";
				reg = <0x1fe0000 0x020000>;
				read-only;
			};
		};
	};

	leds {
		compatible = "gpio-leds";

		usb {
			label = "buffalo:blue:usb";
			gpios = <&gpio 0 GPIO_ACTIVE_LOW>;
			trigger-sources = <&hub_port>;
			linux,default-trigger = "usbport";
		};

		diag: diag {
			label = "buffalo:red:diag";
			gpios = <&gpio 1 GPIO_ACTIVE_LOW>;
		};

		wireless {
			label = "buffalo:green:wireless";
			gpios = <&gpio 6 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0tpt";
		};

		security: security {
			label = "buffalo:orange:security";
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
		};

		router {
			label = "buffalo:green:router";
			gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
		};
	};

	switch {
		compatible = "realtek,rtl8366s";
		gpio-sda = <&gpio 19 GPIO_ACTIVE_HIGH>;
		gpio-sck = <&gpio 20 GPIO_ACTIVE_HIGH>;

		mii-bus = <&mdio0>;

		mdio-bus {
			#address-cells = <1>;
			#size-cells = <0>;
			status = "okay";

			phy-mask = <0x10>;

			phy4: ethernet-phy@4 {
				reg = <4>;
				phy-mode = "rgmii";
			};
		};
	};
};

&eth0 {
	pll-data = <0x1e000100 0x13000a44 0x00441099>;
	status = "okay";

	phy-mode = "rgmii";
	mtd-mac-address = <&art 0x1120c>;

	fixed-link {
		speed = <1000>;
		full-duplex;
	};
};

&eth1 {
	compatible = "qca,ar9130-eth", "syscon";
	reg = <0x1a000000 0x200 0x18070004 0x4>;
	pll-reg = <0x4 0x18 22>;
	pll-handle = <&pll>;
	pll-data = <0x1e000100 0x13000a44 0x00441099>;

	resets = <&rst 13>;
	reset-names = "mac";

	qca,mac-idx = <1>;

	phy-mode = "rgmii";
	phy-handle = <&phy4>;

	mtd-mac-address = <&art 0x1120c>;
	mtd-mac-address-increment = <1>;

	status = "okay";
};

&wmac {
	status = "okay";
	mtd-cal-data = <&art 0x11000>;
	mtd-mac-address = <&art 0x1120c>;
};

&uart {
	status = "okay";
};

&pll {
	clocks = <&clock40mhz>;
};

&usb {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";

	hub_port: port@1 {
		reg = <1>;
		#trigger-source-cells = <0>;
	};
};

&usb_phy {
	status = "okay";
};

&mdio0 {
	status = "okay";
};

&mdio1 {
	status = "okay";
};

