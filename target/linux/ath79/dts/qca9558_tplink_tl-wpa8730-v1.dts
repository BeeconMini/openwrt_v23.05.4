#include "qca955x.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "tplink,tl-wpa8730-v1", "qca,qca9558";
	model = "TP-Link WPA8730 v1";

	aliases {
		label-mac-device = &eth0;

		led-boot = &led_power;
		led-failsafe = &led_power;
		led-running = &led_power;
		led-upgrade = &led_power;
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "Reset button";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};

		leds {
			label = "LED control button";
			linux,code = <KEY_LIGHTS_TOGGLE>;
			gpios = <&gpio 19 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};

		pair {
			label = "Pair button";
			linux,code = <BTN_1>;
			gpios = <&gpio 15 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};

		wifi {
			label = "WiFi button";
			linux,code = <KEY_RFKILL>;
			gpios = <&gpio 20 GPIO_ACTIVE_LOW>;
			debounce-interval = <60>;
		};
	};

	leds {
		compatible = "gpio-leds";

		led_power: power {
			label = "green:power";
			gpios = <&gpio 12 GPIO_ACTIVE_LOW>;
		};

		wifi2g {
			label = "green:wifi2g";
			gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy1tpt";
		};

		wifi5g {
			label = "green:wifi5g";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "phy0tpt";
		};
	};

	gpio-export {
		compatible = "gpio-export";

		led_control {
			gpio-export,name = "led-control";
			gpio-export,output = <0>;
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
		};
	};

	mdio2: mdio {
		compatible = "virtual,mdio-gpio";

		gpios = <&gpio 3 GPIO_ACTIVE_HIGH>, /* MDC */
			<&gpio 1 GPIO_ACTIVE_HIGH>; /* MDIO */
		#address-cells = <1>;
		#size-cells = <0>;

		phy4: ethernet-phy@4 {
			reg = <4>;
		};
	};
};

&pcie0 {
	status = "okay";
};

&uart {
	status = "okay";
};

&spi {
	status = "okay";

	flash@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <25000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			uboot: partition@0 {
				label = "u-boot";
				reg = <0x000000 0x010000>;
				read-only;
			};

			partition@10000 {
				compatible = "tplink,firmware";
				label = "firmware";
				reg = <0x010000 0x7e0000>;
			};

			art: partition@7f0000 {
				label = "art";
				reg = <0x7f0000 0x010000>;
				read-only;
			};
		};
	};
};

&eth0 {
	status = "okay";

	phy-handle = <&phy4>;
	pll-data = <0xa6000000 0x00000101 0x00001616>;

	nvmem-cells = <&macaddr_uboot_fc00>;
	nvmem-cell-names = "mac-address";
};

&wmac {
	status = "okay";

	mtd-cal-data = <&art 0x1000>;

	nvmem-cells = <&macaddr_uboot_fc00>;
	nvmem-cell-names = "mac-address";
};

&uboot {
	compatible = "nvmem-cells";
	#address-cells = <1>;
	#size-cells = <1>;

	macaddr_uboot_fc00: macaddr@fc00 {
		reg = <0xfc00 0x6>;
	};
};
