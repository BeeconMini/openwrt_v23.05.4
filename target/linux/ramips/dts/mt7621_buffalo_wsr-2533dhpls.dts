#include "mt7621.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/leds/common.h>

/ {
	compatible = "buffalo,wsr-2533dhpls", "mediatek,mt7621-soc";
	model = "Buffalo WSR-2533DHPLS";

	chosen {
		bootargs = "console=ttyS0,115200n8";
	};

	aliases {
		led-boot = &led_power;
		led-failsafe = &led_diag;
		led-running = &led_power;
		led-upgrade = &led_power;
	};

	leds {
		compatible = "gpio-leds";
		router_red {
			label = "red:router";
			gpios = <&gpio 4 GPIO_ACTIVE_LOW>;
		};
		wireless_red {
			label = "red:wireless";
			gpios = <&gpio 8 GPIO_ACTIVE_LOW>;
		};
		internet_red {
			label = "red:internet";
			gpios = <&gpio 10 GPIO_ACTIVE_LOW>;
		};
		internet_green {
			color = <LED_COLOR_ID_GREEN>;
			label = "green:internet";
			gpios = <&gpio 12 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "mt7530-0:04:1Gbps";
		};

		led_power: power_green {
			function = LED_FUNCTION_POWER;
			color = <LED_COLOR_ID_GREEN>;
			label = "green:power";
			gpios = <&gpio 13 GPIO_ACTIVE_LOW>;
		};

		led_diag: power_red {
			label = "red:power";
			gpios = <&gpio 14 GPIO_ACTIVE_LOW>;
		};
		wireless_green {
			function = LED_FUNCTION_WLAN;
			color = <LED_COLOR_ID_GREEN>;
			label = "green:wireless";
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
		};
		router_green {
			color = <LED_COLOR_ID_GREEN>;
			label = "green:router";
			gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
		};

	};

	keys: keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			gpios = <&gpio 3 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};

		aoss {
			label = "aoss";
			gpios = <&gpio 6 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_WPS_BUTTON>;
		};

		router {
			label = "router";
			gpios = <&gpio 7 GPIO_ACTIVE_LOW>;
			linux,code = <BTN_0>;
			linux,input-type = <EV_SW>;
		};
	};
};


&ethernet {
	status = "okay";
};

&gmac1 {
	status = "okay";
	label = "wan";
	phy-handle = <&ethphy0>;
};

&mdio {
    ethphy0: ethernet-phy@0 {
        reg = <0x0>;
    };
};


&switch0 {
	ports {
		port@1 {
			status = "okay";
		};

		port@2 {
			status = "okay";
		};

		port@3 {
			status = "okay";
		};

	};
};

&pcie {
	status = "okay";
};

&pcie0 {
	mt76@0,0 {
		reg = <0x0000 0 0 0 0>;
		nvmem-cells = <&eeprom_factory_8000>;
		nvmem-cell-names = "eeprom";
		ieee80211-freq-limit = <2400000 2500000>;
	};
};

&pcie1 {
	mt76@0,0 {
		reg = <0x0000 0 0 0 0>;
		nvmem-cells = <&eeprom_factory_0>;
		nvmem-cell-names = "eeprom";
		ieee80211-freq-limit = <5000000 6000000>;
	};
};

&xhci {
	status = "okay";
};

&nand {
	status = "okay";
	
	partitions {
		compatible = "fixed-partitions";
		#address-cells = <1>;
		#size-cells = <0x01>;

		partition@0 {
			label = "uboot";
			reg = <0x00 0x80000>;
			read-only;
		};

		partition@80000 {
			label = "uboot-env";
			reg = <0x80000 0x80000>;
		};

		partition@100000 {
			label = "Factory";
			reg = <0x100000 0x80000>;
			read-only;
			nvmem-layout {
				compatible = "fixed-layout";
				#address-cells = <1>;
				#size-cells = <1>;

				eeprom_factory_0: eeprom@0 {
					reg = <0x0 0x400>;
				};

				eeprom_factory_8000: eeprom@8000 {
					reg = <0x8000 0x200>;
				};

			};

		};

		partition@180000 {
			compatible = "brcm,trx";
			brcm,trx-magic = <0x534c4844>;
			label = "firmware";
			reg = <0x180000 0x1800000>;
		};

		partition@1980000 {
			label = "Kernel2";
			reg = <0x1980000 0x1800000>;
		};

		partition@3180000 {
			label = "glbcfg";
			reg = <0x3180000 0x200000>;
		};

		partition@3380000 {
			label = "board_data";
			reg = <0x3380000 0x200000>;
			read-only;
		};

		partition@3580000 {
			label = "debug_log";
			reg = <0x3580000 0x900000>;
		};

		partition@3e80000 {
			label = "WTB";
			reg = <0x3e80000 0x3600000>;
		};
	};
};

&state_default {
    gpio {
        groups = "i2c", "uart2", "uart3";
        function = "gpio";
    };
};

