/dts-v1/;
#include <dt-bindings/input/input.h>
#include <dt-bindings/gpio/gpio.h>

#include "mt7628an.dtsi"

/ {
	compatible = "zbtlink,zbt-we2802d", "mediatek,mt7628an-soc";
	model = "ZBT-WE2802D";

	aliases {
		led-boot = &led_sys;
		led-failsafe = &led_sys;
		led-running = &led_sys;
		led-upgrade = &led_sys;
	};

	chosen {
		bootargs = "console=ttyS1,115200";
	};

	keys {
		compatible = "gpio-keys-polled";
		poll-interval = <20>;

		reset {
			label = "reset";
			gpios = <&gpio1 6 GPIO_ACTIVE_LOW>;
			linux,code = <KEY_RESTART>;
		};
	};

	leds {
		compatible = "gpio-leds";

		led_sys: sys {
			label = "zbt-we2802d:green:sys";
			gpios = <&gpio0 4 GPIO_ACTIVE_LOW>;
		};

		led_wan: wan {
			label = "zbt-we2802d:green:wan";
			gpios = <&gpio0 5 GPIO_ACTIVE_LOW>;
		};

		led_lan: lan {
			label = "zbt-we2802d:green:lan";
			gpios = <&gpio0 11 GPIO_ACTIVE_LOW>;
		};

		led_wlan: wlan {
			label = "zbt-we2802d:green:wlan";
			gpios = <&gpio1 12 GPIO_ACTIVE_LOW>;
		};

		led_hwdt: hwdt {
			label = "zbt-we2802d:green:hwdt";
			gpios = <&gpio1 5 GPIO_ACTIVE_LOW>;
			linux,default-trigger = "timer";
	        };
	};
};

&pinctrl {
	state_default: pinctrl0 {
		gpio {
			ralink,group = "p0led_an", "p1led_an", "p4led_an", "wdt", "wled_an";
			ralink,function = "gpio";
		};
	};
};

&spi0 {
	status = "okay";

	m25p80@0 {
		compatible = "jedec,spi-nor";
		reg = <0>;
		spi-max-frequency = <10000000>;

		partitions {
			compatible = "fixed-partitions";
			#address-cells = <1>;
			#size-cells = <1>;

			partition@0 {
				label = "u-boot";
				reg = <0x0 0x30000>;
				read-only;
			};

			partition@30000 {
				label = "u-boot-env";
				reg = <0x30000 0x10000>;
				read-only;
			};

			factory: partition@40000 {
				label = "factory";
				reg = <0x40000 0x10000>;
				read-only;
			};

			partition@50000 {
				compatible = "denx,uimage";
				label = "firmware";
				reg = <0x50000 0x7b0000>;
			};
		};
	};
};

&wmac {
	status = "okay";
	ralink,mtd-eeprom = <&factory 0x4>;
};

&ethernet {
	mtd-mac-address = <&factory 0x2e>;
};
