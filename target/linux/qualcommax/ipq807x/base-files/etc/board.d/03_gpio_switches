
. /lib/functions/uci-defaults.sh

board_config_update

board=$(board_name)

case "$board" in
tplink,x80-5g)
	ucidef_add_gpio_switch "w_disable" "W_DISABLE WWAN POWER CTRL" "547" "1"
	ucidef_add_gpio_switch "modem-reset" "MODEM_RESET CTRL" "541" "0"
	ucidef_add_gpio_switch "ext_antenna_en" "EXT_ANTENNA_ENABLE CTRL" "568" "0"
	;;
esac

board_config_flush

exit 0
